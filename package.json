    "@types/jest": "^24.0.9",
    "@types/lodash.throttle": "^4.1.5",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.9.5",
    "@types/parcel-bundler": "^1.10.2",
    "@types/react": "^16.8.5",
    "@types/supertest": "^2.0.7",
    "@types/ws": "^6.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.14.1",
